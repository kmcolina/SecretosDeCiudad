<div class="banner-bookings">
  <h1>My Bookings</h1>
</div>


<div class="container" >
  <div class="cards">
    <% @bookings.each do |booking| %>
      <div class="card">
        <div class="card-body">

          <p style="margin-top: 15px"><strong><%= booking.route.name %></strong></p>
          <p><strong>Address: </strong><%= booking.route.address %></p>
          <p><strong>Check-In: </strong><%= booking.checkin %></p>
        </div>
      </div>
    <% end %>
  </div>
</div>
