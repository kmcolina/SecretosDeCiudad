<% if user_signed_in? %>
  <%= render "routes/partials/admin" if current_user.admin? %>
  <%= render "routes/partials/guide" if current_user.guide? %>
  <%= render "routes/partials/common" if !current_user.admin? && !current_user.guide? %>
  <%else%>
  <%= render "routes/partials/common" %>
<% end %>
